Title = {{Watset: Automatic Induction of Synsets from a Graph of Synonyms}},
Booktitle = {{PROCEEDINGS OF THE 55TH ANNUAL MEETING OF THE ASSOCIATION FOR
   COMPUTATIONAL LINGUISTICS (ACL 2017), VOL 1}},
Year = {{2017}},
Pages = {{1579-1590}},
