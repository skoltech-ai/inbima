Title = {{Making Fast Graph-based Algorithms with Graph Metric Embeddings}},
Booktitle = {{57TH ANNUAL MEETING OF THE ASSOCIATION FOR COMPUTATIONAL LINGUISTICS
   (ACL 2019)}},
Year = {{2019}},
Pages = {{3349-3355}},
