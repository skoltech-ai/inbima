@article{ WOS:000529307100003,
Author = {Schutski, Roman and Lykov, Danil and Oseledets, Ivan},
Title = {{Adaptive algorithm for quantum circuit simulation}},
Journal = {{PHYSICAL REVIEW A}},
Year = {{2020}},
Volume = {{101}},
Number = {{4}},
Month = {{APR 29}},
Abstract = {{Efficient simulation of quantum computers is essential for the
   development and validation of near-term quantum devices and the research
   on quantum algorithms. Up to date, two main approaches to simulation
   have been use, based on either full-state or single-amplitude
   evaluation. We propose an algorithm that efficiently interpolates
   between these two possibilities. Our approach elucidates the connection
   between quantum circuit simulation and partial evaluation of expressions
   in tensor algebra.}},
