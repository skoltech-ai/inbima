@inproceedings{ WOS:000678697001182,
Author = {Zhang, Xinru and Jin, Jing and Xu, Ren and Miao, Yangyang and Cichocki,
   Andrzej},
Book-Group-Author = {{IEEE}},
Title = {{Effects of the combination of block-shape and face flashing stimuli on a
   P300 brain computer interface}},
Booktitle = {{2020 CHINESE AUTOMATION CONGRESS (CAC 2020)}},
Series = {{Chinese Automation Congress}},
Year = {{2020}},
Pages = {{1898-1904}},
Note = {{Chinese Automation Congress (CAC), Shanghai, PEOPLES R CHINA, NOV 06-08,
   2020}},
Organization = {{IEEE; CAA; IEEE Syst, Man, \& Cybernet Soc}},
Abstract = {{Brain-computer interface (BCI) provides a new way for the brain to
   interact with the outside world. One of the common BCI systems is the
   visual P300 speller. Previous studies have shown that faces can induce
   strong event-related potentials (ERPs) to achieve high classification
   accuracy and the block-shaped condition can obtain better performance by
   increasing the size of the highlighted surface. However, the effects of
   both blended factors have not yet been studied. Therefore, we proposed a
   red block-shaped face paradigm (RBFP) combining both block-shape and
   face. We compared the proposed paradigm with the traditional red face
   paradigm (RFP) to validate its feasibility and effectiveness. Results
   indicated that RBFP had better classification accuracy (95.27\% vs.
   91.116/0) and raw bit rate (R BR) (25.84 bit/min vs. 23.43 bit/min) than
