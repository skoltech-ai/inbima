@article{ WOS:000454425700003,
Author = {Komorowski, Michal K. and Aghabeig, Mansoureh and Nikadon, Jan and
   Piotrowski, Tomasz and Dreszer, Joanna and Balaj, Bibianna and
   Lewandowska, Monika and Wojciechowski, Jakub and Pawlaczyk, Natalia and
   Szmytke, Magdalena and Cichocki, Andrzej and Duch, Wlodzislaw},
Title = {{MULTI-LEVEL EXPLANATIONS IN NEUROSCIENCE II: EEG SPECTRAL FINGERPRINTS
   AND TENSOR DECOMPOSITIONS FOR UNDERSTANDING BRAIN ACTIVITY - INITIAL
   RESULTS}},
Journal = {{ACTA PHYSICA POLONICA B}},
