@article{ WOS:000458495100002,
Author = {Cheng, Jiao and Jin, Jing and Daly, Ian and Zhang, Yu and Wang, Bei and
   Wang, Xingyu and Cichocki, Andrzej},
Title = {{Effect of a combination of flip and zooming stimuli on the performance
   of a visual brain-computer interface for spelling}},
