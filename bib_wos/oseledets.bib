@article{ WOS:000448861300002,
Author = {Kolesnikov, D. A. and Oseledets, I. V.},
Title = {{Convergence analysis of projected fixed-point iteration on a low-rank
   matrix manifold}},
Journal = {{NUMERICAL LINEAR ALGEBRA WITH APPLICATIONS}},
Year = {{2018}},
Volume = {{25}},
Number = {{5}},
Month = {{OCT}},
Abstract = {{In this paper, we analyze the convergence of a projected fixed-point
   iteration on a Riemannian manifold of matrices with fixed rank. As a
   retraction method, we use the projector splitting scheme. We prove that
   the convergence rate of the projector splitting scheme is bounded by the
   convergence rate of standard fixed-point iteration without rank
   constraints multiplied by the function of initial approximation. We also
   provide counterexample to the case when conditions of the theorem do not
   hold. Finally, we support our theoretical results with numerical
   experiments.}},
