@article{ WOS:000436986000043,
Author = {Sushnikova, Daria A. and Oseledets, V, Ivan},
Title = {{``COMPRESS AND ELIMINATE{''} SOLVER FOR SYMMETRIC POSITIVE DEFINITE
   SPARSE MATRICES}},
Journal = {{SIAM JOURNAL ON SCIENTIFIC COMPUTING}},
Year = {{2018}},
Volume = {{40}},
Number = {{3}},
Pages = {{A1742-A1762}},
Abstract = {{We propose a new approximate factorization for solving linear systems
   with symmetric positive definite sparse matrices. In a nutshell the
