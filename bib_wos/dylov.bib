@inproceedings{ WOS:000653607600060,
Author = {Leli, Vito M. and Rubashevskii, Aleksandr and Sarachakov, Aleksandr and
   Rogov, Oleg and Dylov, V, Dmitry},
Book-Group-Author = {{IEEE}},
Title = {{Near-Infrared-to-Visible Vein Imaging via Convolutional Neural Networks
   and Reinforcement Learning}},
Booktitle = {{16TH IEEE INTERNATIONAL CONFERENCE ON CONTROL, AUTOMATION, ROBOTICS AND
   VISION (ICARCV 2020)}},
Series = {{International Conference on Control Automation Robotics and Vision}},
Year = {{2020}},
Pages = {{434-441}},
