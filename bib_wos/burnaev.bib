@article{ WOS:000407425000011,
Author = {Zaytsev, A. and Burnaev, E.},
Title = {{Large scale variable fidelity surrogate modeling}},
Journal = {{ANNALS OF MATHEMATICS AND ARTIFICIAL INTELLIGENCE}},
Year = {{2017}},
Volume = {{81}},
