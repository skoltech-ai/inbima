Author = {Velichkovsky, Boris B. and Khromov, Nikita and Korotin, Alexander and
   Burnaev, Evgeny and Somov, Andrey},
Editor = {{Lamas, D and Loizides, F and Nacke, L and Petrie, H and Winckler, M and Zaphiris, P}},
Title = {{Visual Fixations Duration as an Indicator of Skill Level in eSports}},
