@inproceedings{ WOS:000476898200025,
Author = {Smolyakov, Dmitry and Sviridenko, Nadezda and Burikov, Evgeny and
   Burnaev, Evgeny},
Editor = {{Pancioni, L and Schwenker, F and Trentin, E}},
Title = {{Anomaly Pattern Recognition with Privileged Information for Sensor Fault
   Detection}},
