@inproceedings{ WOS:000611776800038,
Author = {Bokhovkin, Alexey and Burnaev, Evgeny},
Editor = {{Lu, H and Tang, H and Wang, Z}},
Title = {{Boundary Loss for Remote Sensing Imagery Semantic Segmentation}},
