@inproceedings{ WOS:000477731600040,
Author = {Li, Yuchen and Fan, Ju and Ovchinnikov, George V. and Karras, Panagiotis},
Book-Group-Author = {{IEEE}},
Title = {{Maximizing Multifaceted Network Influence}},
Booktitle = {{2019 IEEE 35TH INTERNATIONAL CONFERENCE ON DATA ENGINEERING (ICDE 2019)}},
Series = {{IEEE International Conference on Data Engineering}},
Year = {{2019}},
Pages = {{446-457}},
Note = {{IEEE 35th International Conference on Data Engineering (ICDE), Macau,
   PEOPLES R CHINA, APR 08-11, 2019}},
Organization = {{IEEE; IEEE Comp Soc}},
Abstract = {{An information dissemination campaign is often multifaceted, involving
   several facets or pieces of information disseminating from different
   sources. The question then arises, how should we assign such pieces to
   eligible sources so as to achieve the best viral dissemination results?
   Past research has studied the problem of Influence Maximization (IM),
   which is to select a set of k promoters that maximizes the expected
   reach of a message over a network. However, in this classical IM
   problem, each promoter spreads out the same unitary piece of
   information.
   In this paper, we propose the Optimal Influential Pieces Assignment
   (0171),A) problem, which is to assign k distinct pieces of an
   information campaign T to k promoters, so as to achieve the highest
   viral adoption in a network. We express adoption by users with a
   logistic model, and show that approximating OIPA within any constant
   factor is NP-hard. Even so, we propose a branch and-hound framework for
   OIPA with an (1 - 1/e) approximation ratio. We further optimize this
   framework with a pruning intensive progressive upper-bound estimation
   approach, yielding a (1 - 1/e - epsilon) approximation ratio and
   significantly lower time complexity, as it relies on the power-law
   properties of real-world social networks to run efficiently. Our
   extensive experiments on several real-world datasets show that the
   proposed approaches consistently outperform intuitive baselines, adopted
   from state-of-the-art IM algorithms. Furthermore, the progressive
   approach demonstrates superior efficiency with an up to 24-fold speedup
   over the plain branch-and-bound approach.}},
