
@article{ WOS:000619235900003,
Author = {Fedorov, Fedor S. and Yaqin, Ainul and Krasnikov, Dmitry V. and
   Kondrashov, Vladislav A. and Ovchinnikov, George and Kostyukevich, Yury
   and Osipenko, Sergey and Nasibulin, Albert G.},
Title = {{Detecting cooking state of grilled chicken by electronic nose and
   computer vision techniques}},
Journal = {{FOOD CHEMISTRY}},
Year = {{2021}},
Volume = {{345}},
Month = {{MAY 30}},
Abstract = {{Determination of food doneness remains a challenge for automation in the
   cooking industry. The complex physicochemical processes that occur
   during cooking require a combination of several methods for their
   control. Herein, we utilized an electronic nose and computer vision to
   check the cooking state of grilled chicken. Thermogravimetry,
   differential mobility analysis, and mass spectrometry were employed to
   deepen the fundamental insights towards the grilling process. The
   results indicated that an electronic nose could distinguish the odor
   profile of the grilled chicken, whereas computer vision could identify
   discoloration of the chicken. The integration of these two methods
   yields greater selectivity towards the qualitative determination of
   chicken doneness. The odor profile is matched with detected water loss,
   and the release of aromatic and sulfur-containing compounds during
   cooking. This work demonstrates the practicability of the developed
   technique, which we compared with a sensory evaluation, for better
   deconvolution of food state during cooking.}},
