@article{ WOS:000394439100006,
Author = {Liu, Peng and Zeng, Zhigang and Wang, Jun},
Title = {{Multistability of Delayed Recurrent Neural Networks with Mexican Hat
   Activation Functions}},
Journal = {{NEURAL COMPUTATION}},
Year = {{2017}},
Volume = {{29}},
Number = {{2}},
Pages = {{423-457}},
Month = {{FEB}},
Abstract = {{This letter studies the multistability analysis of delayed recurrent
   neural networks with Mexican hat activation function. Some sufficient
   conditions are obtained to ensure that an n-dimensional recurrent neural
   network can have 3(k1)5(k2) equilibrium points with 0 <= k(1) + k(2) <=
   n, and 2(k1)3(k2) of them are locally exponentially stable. Furthermore,
   the attraction basins of these stable equilibrium points are estimated.
   We show that the attraction basins of these stable equilibrium points
   can be larger than their originally partitioned subsets. The results of
   this letter improve and extend the existing stability results in the
   literature. Finally, a numerical example containing different cases is
   given to illustrate the theoretical results.}},
