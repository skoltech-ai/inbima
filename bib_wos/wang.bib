@article{ WOS:000641162100004,
Author = {Che, Hangjun and Wang, Jun},
Title = {{A Two-Timescale Duplex Neurodynamic Approach to Mixed-Integer
   Optimization}},
Journal = {{IEEE TRANSACTIONS ON NEURAL NETWORKS AND LEARNING SYSTEMS}},
Year = {{2021}},
Volume = {{32}},
Number = {{1}},
Pages = {{36-48}},
Month = {{JAN}},
Abstract = {{This article presents a two-timescale duplex neurodynamic approach to
   mixed-integer optimization, based on a biconvex optimization problem
   reformulation with additional bilinear equality or inequality
   constraints. The proposed approach employs two recurrent neural networks
   operating concurrently at two timescales. In addition, particle swarm
   optimization is used to update the initial neuronal states iteratively
   to escape from local minima toward better initial states. In spite of
   its minimal system complexity, the approach is proven to be almost
   surely convergent to optimal solutions. Its superior performance is
   substantiated via solving five benchmark problems.}},
