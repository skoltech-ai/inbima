@article{ WOS:000390251700004,
Author = {Liu, Peng and Zeng, Zhigang and Wang, Jun},
Title = {{Complete stability of delayed recurrent neural networks with Gaussian
   activation functions}},
Journal = {{NEURAL NETWORKS}},
Year = {{2017}},
Volume = {{85}},
Pages = {{21-32}},
Month = {{JAN}},
Abstract = {{This paper addresses the complete stability of delayed recurrent neural
   networks with Gaussian activation functions. By means of the geometrical
   properties of Gaussian function and algebraic properties of nonsingular
   M-matrix, some sufficient conditions are obtained to ensure that for an
   n-neuron neural network, there are exactly 3(k) equilibrium points with
   0 <= k <= n, among which 2(k) and 3(k) - 2(k) equilibrium points are
   locally exponentially stable and unstable, respectively. Moreover, it
   concludes that all the states converge to one of the equilibrium points;
   i.e., the neural networks are completely stable. The derived conditions
   herein can be easily tested. Finally, a numerical example is given to
   illustrate the theoretical results. (C) 2016 Elsevier Ltd. All rights
   reserved.}},
